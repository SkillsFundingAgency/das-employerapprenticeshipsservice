@model SFA.DAS.EAS.Web.OrchestratorResponse

<div class="grid-row">
    <div class="column-two-thirds">
        <h1 class="heading-xlarge access-denied">Access denied</h1>
        <p class="access-denied">Your account does not have sufficient privileges</p>
    </div>
</div>

<div class="grid-row">
    <div class="column-two-thirds">
        <p class="">
            If you are experiencing difficulty accessing the area of the site you need, first contact an/the account owner to ensure you have the correct role assigned to your account.
        </p>
        @if (!string.IsNullOrWhiteSpace(Model?.RedirectUrl))
        {
            <a class="button" href="@Model.RedirectUrl">@Model.FlashMessage.RedirectButtonMessage</a>

        }
        else if (ViewBag.AccountId != null)
        {
            <a class="button" href="@Url.Action("Index", "EmployerTeam", new {accountId = ViewBag.AccountId})">Go back to the account home page</a>
        }
        else
        {
            <a class="button" href="/">Go back to the service home page</a>
        }
    </div>
</div>
