            </div>
            <div class="govuk-header__content">
                <a href="/" class="govuk-header__link govuk-header__link--service-name">
                    Manage apprenticeships
                </a>
            </div>
        </div>
    </header>

    <div class="das-account-header das-account-header--employer">
        <div class="govuk-width-container">
            <p class="das-account-header__title">Your employer account</p>

            @if (HttpContext.Current.User.Identity.IsAuthenticated || Model?.GetType().GetProperty("HideHeaderSignInLink") == null)
            {
                <nav class="das-user-navigation" id="das-user-navigation">
                    <ul class="das-user-navigation__list" role="menu">
                        <li role="menuitem" class="das-user-navigation__list-item">
                            <a href="@Url.Action(ControllerConstants.HelpActionName, ControllerConstants.HomeControllerName)" class="das-user-navigation__link">Help</a>
                        </li>
                        @if (HttpContext.Current.User.Identity.IsAuthenticated)
                        {
                            <li role="menuitem" class="das-user-navigation__list-item das-user-navigation__list-item--has-sub-menu">
                                <a href="/" class="das-user-navigation__link">Settings</a>
                                <ul role="menu" class="das-user-navigation__sub-menu js-hidden">
                                    @if (ViewBag.HideNav == null || !ViewBag.HideNav)
                                    {
                                        <li class="das-user-navigation__sub-menu-list-item" role="menuitem">
                                            <a href="@Url.Action(ControllerConstants.ViewAccountsActionName, ControllerConstants.HomeControllerName)" class="das-user-navigation__sub-menu-link">Your accounts</a>
                                        </li>
                                        <li class="das-user-navigation__sub-menu-list-item" role="menuitem">
                                            <a href="@Url.Action(ControllerConstants.RenameAccountActionName, ControllerConstants.EmployerAccountControllerName)" class="das-user-navigation__sub-menu-link">Rename account</a>
                                        </li>
                                    }
                                    <li class="das-user-navigation__sub-menu-list-item" role="menuitem">
                                        <a href="@UserLinksViewModel.ChangePasswordLink" class="das-user-navigation__sub-menu-link">Change your password</a>
                                    </li>
                                    <li class="das-user-navigation__sub-menu-list-item" role="menuitem">
                                        <a href="@UserLinksViewModel.ChangeEmailLink" class="das-user-navigation__sub-menu-link">Change your email address</a>
                                    </li>
                                    @if (ViewBag.HideNav == null || !ViewBag.HideNav)
                                    {
                                        <li class="das-user-navigation__sub-menu-list-item" role="menuitem">
                                            <a href="@Url.Action("NotificationSettings", "Settings")" class="das-user-navigation__sub-menu-link">Notification settings</a>
                                        </li>
                                    }
                                </ul>
                            </li>
                            <li role="menuitem" class="das-user-navigation__list-item">
                                <a href="@Url.Action(ControllerConstants.SignOutActionName, ControllerConstants.HomeControllerName)" class="das-user-navigation__link">Sign out</a>
                            </li>
                        }
                        else if (HttpContext.Current.User.Identity.IsAuthenticated || Model?.GetType().GetProperty("HideHeaderSignInLink") == null)
                        {
                            {
                                <li role="menuitem" class="das-user-navigation__list-item">
                                    <a href="@Url.Action(ControllerConstants.SignInActionName, ControllerConstants.HomeControllerName)" class="das-user-navigation__link">Sign in / Register</a>
                                </li>
                            }
                        }
                    </ul>
                </nav>
            }

        </div>
    </div>

    @if (HttpContext.Current.User.Identity.IsAuthenticated)
    {
        var controllerString = ViewContext.RouteData.Values["Controller"].ToString();

        if (ViewBag.HideNav == null || !ViewBag.HideNav)
        {
            <nav class="das-navigation">
                <div class="govuk-width-container">
                    <ul class="das-navigation__list" id="navigation">
                        <li class="das-navigation__list-item">
                            <a class="das-navigation__link @(ViewBag.Section == "home" ? "das-navigation__link--current" : "")" href="@Url.Action(ControllerConstants.IndexActionName, ControllerConstants.EmployerTeamControllerName)">Home</a>
                        </li>

                        <li class="das-navigation__list-item">
                            <a class="das-navigation__link @(ViewBag.Section == "finance" ? "das-navigation__link--current" : "")" href="@Url.EmployerFinanceAction("finance")">Finance</a>
                        </li>
                        @if (Html.IsAuthorized(FeatureType.Recruitments))
                        {
                            <li class="das-navigation__list-item">
                                <a class="das-navigation__link" href="@Url.EmployerRecruitAction()">Recruitment</a>
                            </li>
                        }

                        <li class="das-navigation__list-item">
                            <a class="das-navigation__link @(controllerString == "EmployerCommitments" ? "das-navigation__link--current" : "")" href="@Url.EmployerCommitmentsAction("apprentices/home")">Apprentices</a>
                        </li>
                        <li class="das-navigation__list-item">
                            <a class="das-navigation__link @(ViewBag.Section == "team" ? "das-navigation__link--current" : "")" href="@Url.Action("ViewTeam", "EmployerTeam")">Your team</a>
                        </li>
                        <li class="das-navigation__list-item">
                            <a class="das-navigation__link @(ViewBag.Section == "organisations" ? "das-navigation__link--current" : "")" href="@Url.Action("Index", "EmployerAgreement")">Your organisations and agreements</a>
                        </li>
                        <li class="das-navigation__list-item">
                            <a class="das-navigation__link das-navigation__link--last @(ViewBag.Section == "paye" ? "das-navigation__link--current" : "")" href="@Url.Action("Index", "EmployerAccountPaye")">PAYE schemes</a>
                        </li>
                    </ul>
                </div>
            </nav>
        }
    }

    <div class="govuk-width-container">
        <main class="govuk-main-wrapper " id="main-content" role="main">
            @RenderBody()
        </main>
    </div>

    <footer class="govuk-footer " role="contentinfo">
        <div class="govuk-width-container ">
            <div class="govuk-footer__meta">
                <div class="govuk-footer__meta-item govuk-footer__meta-item--grow">
                    <h2 class="govuk-visually-hidden">Support links</h2>
                    <ul class="govuk-footer__inline-list">
                        <li class="govuk-footer__inline-list-item">
                            <a class="govuk-footer__link" href="@Url.Action("Help", "Home")">
                                Help
                            </a>
                        </li>
                        <li class="govuk-footer__inline-list-item">
                            <a class="govuk-footer__link" href="https://www.smartsurvey.co.uk/s/apprenticeshipservicefeedback/" target="_blank">
                                Feedback
                            </a>
                        </li>
                        <li class="govuk-footer__inline-list-item">
                            <a class="govuk-footer__link" href="@Url.Action("Privacy", "Home")">
                                Privacy and cookies
                            </a>
                        </li>
                        <li class="govuk-footer__inline-list-item">
                            Built by the <a class="govuk-footer__link" href="http://gov.uk/esfa" target="_blank">Education and Skills Funding Agency</a>
                        </li>
                    </ul>
                    <svg role="presentation" focusable="false" class="govuk-footer__licence-logo" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 483.2 195.7" height="17" width="41">
                        <path fill="currentColor" d="M421.5 142.8V.1l-50.7 32.3v161.1h112.4v-50.7zm-122.3-9.6A47.12 47.12 0 0 1 221 97.8c0-26 21.1-47.1 47.1-47.1 16.7 0 31.4 8.7 39.7 21.8l42.7-27.2A97.63 97.63 0 0 0 268.1 0c-36.5 0-68.3 20.1-85.1 49.7A98 98 0 0 0 97.8 0C43.9 0 0 43.9 0 97.8s43.9 97.8 97.8 97.8c36.5 0 68.3-20.1 85.1-49.7a97.76 97.76 0 0 0 149.6 25.4l19.4 22.2h3v-87.8h-80l24.3 27.5zM97.8 145c-26 0-47.1-21.1-47.1-47.1s21.1-47.1 47.1-47.1 47.2 21 47.2 47S123.8 145 97.8 145" />
                    </svg>
                    <span class="govuk-footer__licence-description">
                        All content is available under the
                        <a class="govuk-footer__link" href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence v3.0</a>, except where otherwise stated
                    </span>
                </div>
                <div class="govuk-footer__meta-item">
                    <a class="govuk-footer__link govuk-footer__copyright-logo" href="https://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/uk-government-licensing-framework/crown-copyright/">© Crown copyright</a>
                </div>
            </div>
        </div>
    </footer>
    @if (!string.IsNullOrWhiteSpace(SystemDetailsViewModel.EnvironmentName))
    {
        <p class="govuk-body">
            Environment: @SystemDetailsViewModel.EnvironmentName ● Version: @SystemDetailsViewModel.VersionNumber
        </p>
    }

    <script src="https://das-prd-frnt-end.azureedge.net//libs/govuk-frontend/all.js"></script>

    <script src="https://das-prd-frnt-end.azureedge.net//libs/jquery/jquery.min.js"></script>

    <script src="https://das-prd-frnt-end.azureedge.net//js/das-all.js"></script>
    <script src="https://das-prd-frnt-end.azureedge.net//js/app.min.js"></script>

    <script>
        window.GOVUKFrontend.initAll();
        window.DASFrontend.initAll();
    </script>
    @RenderSection("pageSpecificJS", required: false)

</body>

</html>