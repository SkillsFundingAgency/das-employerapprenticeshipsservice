@model SFA.DAS.EmployerAccounts.Web.ViewModels.PanelViewModel<SFA.DAS.EmployerAccounts.Web.ViewModels.AccountDashboardViewModel>

@if (!Model.ViewName.Equals("Empty"))
{
    <div class="das-panel das-panel--@(Model.PanelType.ToString().ToLower())">
