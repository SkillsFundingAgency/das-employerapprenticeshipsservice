    <variable name="appName" value="das-employerfinance-web"/>
    <variable name="simplelayout" value="${longdate} [${uppercase:${level}}] [${logger}] - ${message} ${onexception:${exception:format=tostring}}"/>
    <targets>
      <target xsi:type="File" name="File" fileName="${basedir}/App_Data/logs/${appName}.${shortdate}.log" layout="${simplelayout}"/>
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="File"/>
    </rules>
  </nlog>
