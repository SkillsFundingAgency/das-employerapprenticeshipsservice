<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SFA.DAS.EmployerApprenticeshipsService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6" upgradeDomainCount="5">
  <WebRole name="SFA.DAS.EAS.Web" vmsize="Standard_D2_v2">
    <Startup>
      <Task commandLine="StartUp\IIS.bat" executionContext="elevated" taskType="background">
      </Task>
    </Startup>
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="HttpsIn" endpointName="HttpsIn" />
          <Binding name="HttpIn" endpointName="HttpIn" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="StorageConnectionString" />
      <Setting name="EnvironmentName" />
      <Setting name="ConfigurationStorageConnectionString" />
      <Setting name="InstrumentationKey" />
      <Setting name="LogLevel" />
      <Setting name="RedisConnection" />
      <Setting name="LoggingRedisConnectionString"/>
      <Setting name="LoggingRedisKey"/>
      <Setting name="TokenCertificateThumbprint"/>
      <Setting name="ShowAgreements" />
      <Setting name="ShowPayeHistory" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="HttpsIn" protocol="https" port="443" certificate="WebSslCert" loadBalancer="das-pp-eas-ilb"/>
      <InputEndpoint name="HttpIn" protocol="http" port="80" loadBalancer="das-pp-eas-ilb"/>
    </Endpoints>
    <Certificates>
      <Certificate name="WebSslCert" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
    <Imports>
    </Imports>
  </WebRole>
  <WorkerRole name="SFA.DAS.EAS.LevyDeclarationProvider.Worker" vmsize="Standard_D1_v2">
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="ConfigurationStorageConnectionString" />
      <Setting name="EnvironmentName" />
      <Setting name="StorageConnectionString" />
      <Setting name="LogLevel" />
      <Setting name="LoggingRedisConnectionString"/>
      <Setting name="LoggingRedisKey"/>
      <Setting name="DeclarationsEnabled" />
      <Setting name="TokenServiceCertificateThumbprint" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Fake" protocol="tcp" port="50121" />
    </Endpoints>
    <LocalResources>
    </LocalResources>
    <Imports>
    </Imports>
  </WorkerRole>
  <WorkerRole name="SFA.DAS.EAS.PaymentProvider.Worker" vmsize="Standard_D1_v2">
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="StorageConnectionString" />
      <Setting name="EnvironmentName" />
      <Setting name="ConfigurationStorageConnectionString" />
      <Setting name="LogLevel" />
      <Setting name="InstrumentationKey" />
      <Setting name="LoggingRedisConnectionString"/>
      <Setting name="LoggingRedisKey"/>
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Fake" protocol="tcp" port="50122" />
    </Endpoints>
  </WorkerRole>
</ServiceDefinition>
